        <record id="seq_ineco_cheque_out" model="ir.sequence">
            <field name="name">เช็คจ่าย</field>
            <field name="code">ineco.cheque.out</field>
            <field name="prefix">CQO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for ineco.cheque.int -->
        <record id="seq_ineco_cheque_int" model="ir.sequence">
            <field name="name">เช็ครับ</field>
            <field name="code">ineco.cheque.in</field>
            <field name="prefix">CQI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_ineco_pay_petty_cash" model="ir.sequence">
            <field name="name">เบิกเงินสดย่อย</field>
            <field name="code">ineco.pay.in.petty.cash</field>
            <field name="prefix">PC</field>
