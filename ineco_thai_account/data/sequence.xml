<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for ineco.billing -->
        <record id="seq_ineco_billing" model="ir.sequence">
            <field name="name">ใบวางบิล</field>
            <field name="code">ineco.billing</field>
            <field name="prefix">BL</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_ineco_billing2" model="ir.sequence">
            <field name="name">ใบวางบิล2</field>
            <field name="code">ineco.billing2</field>
            <field name="prefix">BL2</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for ineco.customer.payment -->
        <record id="seq_ineco_customer_payment" model="ir.sequence">
            <field name="name">รับชำระ</field>
            <field name="code">ineco.customer.payment</field>
            <field name="prefix">RC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for ineco.customer.deposit -->
        <record id="seq_ineco_customer_deposit" model="ir.sequence">
            <field name="name">รับมัดจำ</field>
            <field name="code">ineco.customer.deposit</field>
            <field name="prefix">AI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
