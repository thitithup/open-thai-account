        <!-- Sequences for ineco.supplier.payment -->
        <record id="seq_ineco_supplier_payment" model="ir.sequence">
            <field name="name">จ่ายชำระ</field>
            <field name="code">ineco.supplier.payment</field>
            <field name="prefix">PV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

